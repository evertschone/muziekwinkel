<h1>Editing uitgelicht</h1>

<% form_for(@uitgelicht) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :album_id %><br />
    <%= f.text_field :album_id %>
  </p>
  <p>
    <%= f.label :omschrijving %><br />
    <%= f.text_area :omschrijving %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @uitgelicht %> |
<%= link_to 'Back', uitgelichts_path %>
