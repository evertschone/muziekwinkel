<h1>New uitgelicht</h1>

<% form_for(@uitgelicht) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :album %><br />
    <%= f.collection_select :album_id, Album.find(:all).sort_by {|item| item.titel}, :id, :titel %>
  </p>
  <p>
    <%= f.label :omschrijving %><br />
    <%= f.text_area :omschrijving %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', uitgelichts_path %>
